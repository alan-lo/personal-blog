<div class="like-container container-fluid mt-3-75">

  <div class="row">
    <div class="offset-sm-1 col-sm-10">
      <h2> <i class="fa fa-heart" aria-hidden="true"></i> Liked Posts</h2>
    </div>
  </div>
  <% @posts.each do |p| %>
  <div class="row">
    <div class="offset-sm-1 col">
      <h3><%= link_to post_path(p) do %>
          <%= p.title %>
        <%end %>
      </h3>
    </div>
    <div class="col">
      <h3>Date Created: <%= p.created_at.strftime("%Y-%m-%d") %></h3>
    </div>
  </div>
  <% end %>
</div>
